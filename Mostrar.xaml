        <TextBox x:Name="txtMostrar_gastos" HorizontalAlignment="Left" Margin="640,342,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="135" IsReadOnly="True" FontFamily="Arial Rounded MT Bold"/>
        <Button x:Name="BtnVolver" Content="Volver" HorizontalAlignment="Left" Margin="704,24,0,0" VerticalAlignment="Top" Width="71" Background="#FFCBA2E4" Click="BtnVolver_Click" FontFamily="Arial Rounded MT Bold"/>
        <Label Content="Categoria" HorizontalAlignment="Left" Margin="542,316,0,0" VerticalAlignment="Top" FontFamily="Arial Rounded MT Bold"/>
        <TextBox x:Name="txtmostrar_Categ" HorizontalAlignment="Left" Margin="542,342,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="82" IsReadOnly="True" FontFamily="Arial Rounded MT Bold"/>
        <Label Content="User" HorizontalAlignment="Left" Margin="29,431,0,0" VerticalAlignment="Top" FontFamily="Arial Rounded MT Bold"/>
        <TextBox x:Name="txtmostrar_user" HorizontalAlignment="Left" Margin="72,434,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" IsReadOnly="True" FontFamily="Arial Rounded MT Bold"/>
        <Label Content="Create" HorizontalAlignment="Left" Margin="284,427,0,0" VerticalAlignment="Top" FontFamily="Arial Rounded MT Bold"/>
        <TextBox x:Name="txtmostrar_create" HorizontalAlignment="Left" Margin="338,434,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="169" IsReadOnly="True" FontFamily="Arial Rounded MT Bold"/>
        <Label Content="Update" HorizontalAlignment="Left" Margin="539,431,0,0" VerticalAlignment="Top" FontFamily="Arial Rounded MT Bold"/>
        <TextBox x:Name="txtmostrar_update" HorizontalAlignment="Left" Margin="603,433,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="163" IsReadOnly="True" FontFamily="Arial Rounded MT Bold"/>
        <Label Content="Limitaciones" HorizontalAlignment="Left" Margin="284,383,0,0" VerticalAlignment="Top" FontFamily="Arial Rounded MT Bold" />
        <Label Content="Analfabetismo" HorizontalAlignment="Left" Margin="532,380,0,0" VerticalAlignment="Top" FontFamily="Arial Rounded MT Bold"/>
        <TextBox x:Name="txtmostrar_limitaciones" HorizontalAlignment="Left" Margin="370,386,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="150" IsReadOnly="True" FontFamily="Arial Rounded MT Bold"/>
        <TextBox x:Name="txtmostrar_analfabetismo" HorizontalAlignment="Left" Margin="640,383,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="135" IsReadOnly="True" FontFamily="Arial Rounded MT Bold"/>

    </Grid>
</Window>
